ing\" :style=\"{ color: SidebarFontColor4 }\">\r\n        <i class=\"bi bi-gear\"></i>Setting\r\n      </button>\r\n      <button @click=\"toExit\" :style=\"{ color: ExitButtonColor }\">\r\n        <i class=\"bi bi-box-arrow-left\"></i>Exit\r\n      </button>\r\n    </div>\r\n    <footer class=\"sidebar-footer\">\r\n      <a href=\"https://github.com/Ruzz-GD\" :style=\"{color:fontColor}\">@Developer</a>\r\n    </footer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { App } from '@capacitor/app';\r\nimport { mapGetters } from 'vuex';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      ActiveTheme: localStorage.getItem('theme'),\r\n      SidebarFontColor1: '',\r\n      SidebarFontColor2: '',\r\n      SidebarFontColor3: '',\r\n      SidebarFontColor4: '',\r\n      SidebarFontColor5: '',\r\n      SidebarBg: '',\r\n      defaultFontColorLight: 'black',\r\n      defaultFontColorDark: 'white',\r\n      highlightedColor: 'skyblue',\r\n      fontColor:'',\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters(['currentTheme']),\r\n    ShowSidebar() {\r\n      return this.$store.state.SidebarAnimation;\r\n    },\r\n    ActiveSidebar() {\r\n      return this.$store.state.ActiveSidebar;\r\n    },\r\n    SidebarFontColor() {\r\n      return this.currentTheme === 'dark' ? this.defaultFontColorDark : this.defaultFontColorLight;\r\n    },\r\n    ExitButtonColor() {\r\n      return this.currentTheme === 'dark' ? this.defaultFontColorDark : this.defaultFontColorLight;\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    currentRoute() {\r\n      const CurrentRoute = sessionStorage.getItem('current-route');\r\n      const fontColor = this.SidebarFontColor;\r\n      this.fontColor = this.currentTheme === 'dark' ? 'white' : 'black';\r\n      this.SidebarFontColor1 = CurrentRoute === 'home' ? this.highlightedColor : fontColor;\r\n      this.SidebarFontColor2 = CurrentRoute === 'account' ? this.highlightedColor : fontColor;\r\n      this.SidebarFontColor3 = CurrentRoute === 'proglang' ? this.highlightedColor : fontColor;\r\n      this.SidebarFontColor4 = CurrentRoute === 'setting' ? this.highlightedColor : fontColor;\r\n      this.SidebarFontColor5 = CurrentRoute === 'help&support' ? this.highlightedColor : fontColor;\r\n    },\r\n    toHomeMain() {\r\n      sessionStorage.setItem('current-route', 'home');\r\n      this.$router.push('/home');\r\n      this.$store.dispatch('offOverlay');\r\n      this.currentRoute();\r\n    },\r\n    toHomeProglang() {\r\n      sessionStorage.setItem('current-route', 'proglang');\r\n      this.$router.push('/language');\r\n      this.$store.dispatch('offOverlay');\r\n      this.currentRoute();\r\n    },\r\n    toHomeSetting() {\r\n      sessionStorage.setItem('current-route', 'setting');\r\n      this.$router.push('/setting');\r\n      this.$store.dispatch('offOverlay');\r\n      this.currentRoute();\r\n    },\r\n    toExit() {\r\n      App.exitApp();\r\n    },\r\n    setSidebarBg() {\r\n      const theme = this.ActiveTheme;\r\n      if (theme === 'dark') {\r\n        this.SidebarBg = 'rgb(100, 100, 100)';\r\n      } else {\r\n\r\n        this.SidebarBg = 'rgb(220, 220, 220)';\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.currentRoute();\r\n    this.setSidebarBg();\r\n\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.user-profile {\r\n  border: solid 2px black;\r\n  width: 100px;\r\n  height: 100px;\r\n  border-radius: 50%;\r\n  object-fit: cover;\r\n}\r\n\r\n.home-sidebar {\r\n  border-top-right-radius: 10px;\r\n  border-bottom-right-radius: 10px;\r\n  position: absolute;\r\n  height: 100vh;\r\n  width: calc(100vw - 50px);\r\n  height: calc(100vh - 100px);\r\n  z-index: 10;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  flex-direction: column;\r\n  padding: 50px 0;\r\n  gap: 20px;\r\n  transform: translateX(-100%);\r\n  opacity: 0;\r\n  transition: transform 0.3s ease-in-out;\r\n}\r\n\r\n.home-sidebar.show-sidebar {\r\n  opacity: 1;\r\n  transform: translate(0);\r\n}\r\n\r\n.